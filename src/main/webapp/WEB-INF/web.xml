<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CategoryServlet</servlet-name>
  	<display-name>CategoryServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.CategoryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DepartmentServlet</servlet-name>
  	<display-name>DepartmentServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.DepartmentServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Student_registrationServlet</servlet-name>
  	<display-name>Student_registrationServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.Student_registrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CategoryServlet</servlet-name>
  	<url-pattern>/CategoryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DepartmentServlet</servlet-name>
  	<url-pattern>/DepartmentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Student_registrationServlet</servlet-name>
  	<url-pattern>/Student_registrationServlet</url-pattern>
  </servlet-mapping>
</web-app>
